{{#partial "content"}}


    <p> <h2>Details of Engineer {{engineers.name}}</h2></p>


    {{#if engineers}}

        {{#each}}
        <div class="table-responsive">
            <table class="table table-bordered table-dark table-sm">

                <thead>
                <tr>
                    <th scope="col">#.</th>
                    <th scope="col">EMPLOYEE NO.</th>
                    <th scope="col">NAME</th>
                    <th scope="col">MOBILE NO.</th>


                </tr>
                </thead>



                    <tbody>
                    <tr>

                        <td>{{id}}</td>
                        <td>{{emp_no}}</td>
                        <td>{{name}}</td>
                        <td>{{mobileNo}}</td>


                    </tr>
                    </tbody>
        {{/each}}
        {{else}}
        Engineer unavailable. Please add.
        {{/if}}

            </table>

        </div>


{{/partial}}

{{> layout.hbs}}